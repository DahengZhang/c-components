        "copy-webpack-plugin": "^5.1.1",
        "cross-env": "^6.0.3",
        "css-loader": "^3.4.0",
        "ejs-loader": "^0.3.5",
        "electron": "^7.1.7",
        "electron-builder": "^21.2.0",
        "file-loader": "^5.0.2",
        "html-webpack-plugin": "^3.2.0",
        "mini-css-extract-plugin": "^0.9.0",
        "nodemon": "^2.0.2",
